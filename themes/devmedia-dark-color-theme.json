    "statusBar.noFolderBackground": "#1F2F36",
    "statusBar.debuggingBackground": "#D7840A",
    "titleBar.activeBackground": "#1A1A1A",
    "titleBar.activeForeground": "#F6F7FA",
    "titleBar.inactiveBackground": "#1A1A1A",
    "titleBar.inactiveForeground": "#808080",
    "tab.activeBackground": "#1F1F1F",
    "tab.activeForeground": "#C9F31D",
    "tab.activeBorder": "#C9F31D",
    "tab.inactiveBackground": "#1A1A1A",
    "tab.inactiveForeground": "#808080",
    "tab.border": "#2C2C2C",
    "editorGroupHeader.tabsBackground": "#1A1A1A",
    "editorLineNumber.foreground": "#4a4a4a",
    "editorLineNumber.activeForeground": "#C9F31D",
    "editor.selectionBackground": "#6F9A3550",
    "editor.inactiveSelectionBackground": "#6F9A3530",
    "editor.findMatchBackground": "#C9F31D40",
    "editor.findMatchHighlightBackground": "#C9F31D25",
    "editorCursor.foreground": "#C9F31D",
    "editorWhitespace.foreground": "#404040",
    "editorIndentGuide.background": "#2C2C2C",
    "editorIndentGuide.activeBackground": "#6F9A3570",
    "editorBracketMatch.background": "#6F9A3540",
    "editorBracketMatch.border": "#C9F31D",
    "editorWidget.background": "#1F2F36",
    "editorWidget.border": "#C9F31D",
    "editorSuggestWidget.background": "#1F2F36",
    "editorSuggestWidget.selectedBackground": "#6F9A3540",
    "peekView.border": "#C9F31D",
    "peekViewEditor.background": "#1F1F1F",
    "peekViewResult.background": "#1F2F36",
    "peekViewTitle.background": "#1A1A1A",
    "terminal.background": "#1A1A1A",
    "terminal.foreground": "#F6F7FA",
    "terminal.ansiBlue": "#13ECFF",
    "terminal.ansiCyan": "#13ECFF",
    "terminal.ansiGreen": "#A0F31D",
    "terminal.ansiRed": "#ef4444",
    "terminal.ansiYellow": "#D7840A",
    "terminal.ansiMagenta": "#a855f7",
    "button.background": "#C9F31D",
    "button.foreground": "#1A1A1A",
    "button.hoverBackground": "#A0F31D",
    "input.background": "#1F2F36",
    "input.border": "#2C2C2C",
    "input.foreground": "#F6F7FA",
    "inputOption.activeBorder": "#C9F31D",
    "focusBorder": "#C9F31D",
    "scrollbarSlider.background": "#6F9A3540",
    "scrollbarSlider.hoverBackground": "#6F9A3560",
    "scrollbarSlider.activeBackground": "#6F9A3580",
    "badge.background": "#C9F31D",
    "badge.foreground": "#1A1A1A",
    "progressBar.background": "#C9F31D",
    "panel.background": "#1A1A1A",
    "panel.border": "#2C2C2C",
    "panelTitle.activeBorder": "#C9F31D",
    "panelTitle.activeForeground": "#C9F31D",
    "panelTitle.inactiveForeground": "#808080",
    "notificationCenter.border": "#C9F31D",
    "notifications.background": "#1F2F36",
    "notifications.border": "#C9F31D",
    "gitDecoration.modifiedResourceForeground": "#13ECFF",
    "gitDecoration.deletedResourceForeground": "#ef4444",
    "gitDecoration.untrackedResourceForeground": "#A0F31D",
    "gitDecoration.ignoredResourceForeground": "#6b7280",
    "gitDecoration.conflictingResourceForeground": "#D7840A",
    "breadcrumb.background": "#1A1A1A",
    "breadcrumb.foreground": "#F6F7FA",
    "breadcrumb.focusForeground": "#C9F31D",
    "breadcrumb.activeSelectionForeground": "#C9F31D",
    "breadcrumbPicker.background": "#1F2F36",
    "menu.background": "#1F2F36",
    "menu.foreground": "#F6F7FA",
    "menu.selectionBackground": "#6F9A3540",
    "menu.selectionForeground": "#C9F31D",
    "menubar.selectionBackground": "#6F9A3540",
    "menubar.selectionForeground": "#C9F31D"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6b7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#A0F31D"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template meta.template.expression"
      ],
      "settings": {
        "foreground": "#F6F7FA"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language.numeric"
      ],
      "settings": {
        "foreground": "#D7840A"
      }
    },
    {
      "name": "Built-in constants",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#C9F31D"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#C9F31D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#13ECFF"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#13ECFF"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#6F9A35",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#F6F7FA"
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#93c5fd"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#C9F31D"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#13ECFF"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#6F9A35"
      }
    },
    {
      "name": "CSS IDs",
      "scope": [
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#D7840A"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#93c5fd"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#13ECFF"
      }
    },
    {
      "name": "Python Decorators",
      "scope": [
        "meta.function.decorator.python",
        "entity.name.function.decorator.python"
      ],
      "settings": {
        "foreground": "#C9F31D"
      }
    },
    {
      "name": "Python Self",
      "scope": [
        "variable.language.special.self.python"
      ],
      "settings": {
        "foreground": "#6F9A35",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Java Annotations",
      "scope": [
        "storage.type.annotation.java"
      ],
      "settings": {
        "foreground": "#C9F31D"
      }
    },
    {
      "name": "React JSX Component",
      "scope": [
        "support.class.component.js",
        "support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#6F9A35"
      }
    },
    {
      "name": "SQL Keywords",
      "scope": [
        "keyword.other.DML.sql",
        "keyword.other.DDL.sql"
      ],
      "settings": {
        "foreground": "#C9F31D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#C9F31D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#D7840A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#13ECFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#A0F31D"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#13ECFF"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ef4444",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#D7840A",
        "fontStyle": "italic strikethrough"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#6F9A35"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#D7840A"
      }
    }
  ]
}