  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6b7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#A0F31D"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template meta.template.expression"
      ],
      "settings": {
        "foreground": "#F6F7FA"
      }
    },
    {
      "name": "Template Literal Braces",
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#13ECFF"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language.numeric"
      ],
      "settings": {
        "foreground": "#D7840A"
      }
    },
    {
      "name": "Built-in constants",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#C9F31D"
      }
    },
    {
      "name": "Keywords",
