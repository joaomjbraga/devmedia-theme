  "tokenColors": [
    {
      "name": "Comentários",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation",
        "comment.line.double-dash.sql",
        "comment.block.sql",
        "comment.line.number-sign",
        "comment.line.double-slash"
