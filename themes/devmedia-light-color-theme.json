  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6B7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#16A34A"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template meta.template.expression"
      ],
      "settings": {
        "foreground": "#1F2F36"
      }
    },
    {
      "name": "Template Literal Braces",
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#0891B2"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language.numeric"
      ],
      "settings": {
        "foreground": "#D7840A"
      }
    },
    {
      "name": "Built-in constants",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#9333EA"
