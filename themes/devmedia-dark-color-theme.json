      }
    },
    {
      "name": "Constants & Enum Members",
      "scope": [
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#A8C869"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier",
        "keyword.control.java",
        "keyword.other.java",
        "keyword.control.kotlin",
        "keyword.other.kotlin",
        "keyword.control.php",
        "keyword.other.php",
        "storage.type.php",
        "keyword.control.cs",
        "keyword.other.cs",
        "storage.modifier.static.java"
