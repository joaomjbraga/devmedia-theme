  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#5A6478",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "markup.inline.raw",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#C8A96E"
      }
    },
    {
      "name": "String — RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#A8C869"
      }
    },
    {
      "name": "Template Literal Content",
      "scope": [
        "string.template meta.template.expression",
        "meta.embedded.expression.angular",
        "meta.embedded.expression.vue"
      ],
      "settings": {
        "foreground": "#D8DBE4"
      }
    },
    {
      "name": "Template Literal Braces",
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#8BBF42"
